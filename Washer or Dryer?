from datetime import *

def washer_dryer():
    question = input('Do you wish to get the time for Washer(W) or Dryer(D) ')
    if question == "W":
     washer_time = datetime.now() + timedelta(minutes=30)
     print(washer_time.strftime("%Y-%m-%d %I:%M"))

    elif question =="D":
        dryer_time = datetime.now() + timedelta(minutes=45)
        print(dryer_time.strftime("%Y-%m-%d %I:%M"))

washer_dryer()
