from datetime import datetime, timedelta

def washer_dryer():
    question = input('Do you wish to get the time for Washer or Dryer ')
    if question == "Washer":
       washer_time = datetime.now() + timedelta(minutes=30)
       print (washer_time)
    elif question =="Dryer":
        dryer_time = datetime.now() + timedelta(minutes=45)
        print(dryer_time)

washer_dryer()
